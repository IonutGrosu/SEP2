<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="warehouse.client.views.adminview.AdminViewController">
   <children>
      <HBox layoutY="43.0" prefHeight="357.0" prefWidth="600.0">
         <children>
            <VBox prefHeight="357.0" prefWidth="385.0">
               <children>
                  <ListView fx:id="listEmployees" prefHeight="361.0" prefWidth="382.0" />
               </children></VBox>
            <VBox prefHeight="357.0" prefWidth="222.0">
               <children>
                  <Button fx:id="add" mnemonicParsing="false" onAction="#onAdd" prefHeight="49.0" prefWidth="215.0" text="Add" />
                  <Button fx:id="edit" mnemonicParsing="false" onAction="#onEdit" prefHeight="49.0" prefWidth="215.0" text="Edit" />
                  <Button fx:id="remove" mnemonicParsing="false" onAction="#onRemove" prefHeight="49.0" prefWidth="215.0" text="Remove" />
               </children></VBox>
         </children>
      </HBox>
      <HBox layoutX="1.0" layoutY="1.0" prefHeight="45.0" prefWidth="600.0">
         <children>
            <Button fx:id="logout" mnemonicParsing="false" onAction="#onLogout" prefHeight="44.0" prefWidth="66.0" text="Logout" />
            <Button mnemonicParsing="false" prefHeight="44.0" prefWidth="109.0" text="Go to shops" />
         </children></HBox>
   </children>
</AnchorPane>
